<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ChannelView">
    <sql id="selectCommon">
        select app_ids, channel_nos, phone_nums
    </sql>

    <select id="selectData"  resultType="ChannelView">
        <include refid="selectCommon"/>
        from part_time.channel_view limit 1
    </select>

    <update id="updateData" parameterType="ChannelView">

        UPDATE part_time.channel_view
            SET
            app_ids = #{appIds},
            channel_nos = #{channelNos},
            phone_nums = #{phoneNums}
    </update>


</mapper>